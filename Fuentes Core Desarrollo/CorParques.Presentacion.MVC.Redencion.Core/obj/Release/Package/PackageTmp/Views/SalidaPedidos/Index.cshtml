@model CorParques.Negocio.Entidades.TransladoInventario

@{
    ViewBag.Title = "Salida pedidos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet" />
<link href="~/Vendors/datatables.net-responsive-bs/css/responsive.bootstrap.min.css" rel="stylesheet">
<link href="~/Vendors/datatables.net-scroller-bs/css/scroller.bootstrap.min.css" rel="stylesheet">
<link href="~/Content/jquery-ui.min.css" rel="stylesheet" />
<link href="~/Vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">



<form id="frmTransladoInventario">
    <div class="row ">



        <div class="item col-md-4 col-sm-4 col-xs-4">
            <label class="control-label" for="Password">
                Usuario que recibe:
            </label>
        </div>
        <div class="item col-md-4 col-sm-4 col-xs-4">
            <label class="control-label" for="Password">
                Nombre:
            </label>
        </div>

    </div>
    <div class="row ">

        <div class="item col-md-4 col-sm-4 col-xs-4">
            <input type="password" class="form-control" id="IdUsuario" value="" />
            <input type="hidden" id="hdIdUsuario" value="" />
        </div>
        <div class="item col-md-4 col-sm-4 col-xs-4">
            <label id="NombreUsuario" class="control-label"></label>
        </div>

    </div>
    <br />
  <div class="row">
      <div class="item col-md-3 col-sm-3 col-xs-3">
          <label class="control-label" for="Password">
              Fecha uso:
          </label>
      </div>
      <div class="item col-md-3 col-sm-3 col-xs-3">
          <label class="control-label" for="Password">
              Asesor:
          </label>
      </div>
      <div class="item col-md-3 col-sm-3 col-xs-3">
          <label class="control-label" for="Password">
              Cliente:
          </label>
      </div>
      <div class="item col-md-3 col-sm-3 col-xs-3">
          <label class="control-label" for="Password">
              Estado:
          </label>
      </div>
  </div>
    <div class="row">
        <div class="item col-md-3 col-sm-3 col-xs-3">
            <input type="text" class="form-control filtro" data-id="fechauso" value="" />
        </div>
        <div class="item col-md-3 col-sm-3 col-xs-3">
            <input type="text" class="form-control filtro" data-id="asesor" value="" />
        </div>
        <div class="item col-md-3 col-sm-3 col-xs-3">
            <input type="text" class="form-control filtro" data-id="cliente" value="" />
        </div>
        <div class="item col-md-3 col-sm-3 col-xs-3">
            <input type="text" class="form-control filtro" data-id="estado" value="" />
        </div>
    </div>
    <br />
    <div class="row">
        <a href="#" onclick="Excel()" class="btn btn-success" >Exportar</a>
    </div>
    <div class="row dashboard-widget-content">
        <div id="listView">
            @Html.Partial("_Materiales", (IEnumerable<CorParques.Negocio.Entidades.SolicitudRetorno>)ViewBag.Pedidos)
            
        </div>
    </div>
    <br />
  

    <input type="hidden" id="ListMateriales" />

    @section scripts{
        <script src="~/Vendors/datatables.net/js/jquery.dataTables.min.js"></script>
        <script src="~/Vendors/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
        <script src="~/Vendors/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
        <script src="~/Vendors/datatables.net-responsive-bs/js/responsive.bootstrap.js"></script>
        <script src="~/Vendors/datatables.net-scroller/js/dataTables.scroller.min.js"></script>
        <script src="~/Scripts/jquery-ui.min.js"></script>
        <script src="~/Scripts/jquery-ui.min.autComp.js"></script>
        <script src="~/Vendors/jQuery-Mask-Plugin-master/dist/jquery.mask.min.js"></script>
        <script src="~/Scripts/select2.min.js"></script>
        <script src="~/Scripts/SalidaPedidos.js"></script>
        <script>
            var pedidos = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ViewBag.Pedidos));
            var materiales = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(ViewBag.Inventario));
        </script>
    }
</form>
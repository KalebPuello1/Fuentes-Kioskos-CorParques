@model IEnumerable<CorParques.Negocio.Entidades.RegistroFallas>

<table class="table table-striped  table-bordered" cellspacing="0">
    <thead>
        <tr>
            <th>Taquilla/Almacen</th>
            <th>Referencia</th>
            <th>Responsable</th>
            <th>Cantidad Disponible</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.descripcion</td>
                <td>@item.tecnico</td>
                <td>@item.observacionTecnica</td>
                <td>@item.idArea</td>
            </tr>
        }
    </tbody>
</table>
@model IEnumerable<CorParques.Negocio.Entidades.ReporteBoleteria>
@{
    ViewBag.Title = "Cantidad disponible del día";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-striped  table-bordered" id="tbTrasladoInventario" cellspacing="0">
    <thead>
        <tr>
            <th>Referencia</th>
            <th>Cantidad Impresión</th>
            <th>Cantidad Disponible</th>
            <th>Detalle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.TipoBrazalete</td>
                <td>@item.Redenciones</td>
                <td>@item.TotalVendidos</td>
                <td><a class="lnkView" data-id="@item.Asignados" href="javascript:void(0)"><b class="fa fa-eye"></b></a></td>
            </tr>
        }
    </tbody>
</table>

@section scripts{
    <script src="~/Vendors/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Vendors/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <script src="~/Vendors/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Vendors/datatables.net-responsive-bs/js/responsive.bootstrap.js"></script>
    <script src="~/Vendors/datatables.net-scroller/js/dataTables.scroller.min.js"></script>
    <script src="~/Scripts/jquery-ui.min.js"></script>
    <script src="~/Scripts/InventarioOperacion.js"></script>
}

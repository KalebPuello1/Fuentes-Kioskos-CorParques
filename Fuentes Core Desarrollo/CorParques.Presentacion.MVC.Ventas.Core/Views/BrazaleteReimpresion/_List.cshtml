@model IEnumerable<CorParques.Negocio.Entidades.BrazaleteReimpresion>
<table id="datatable-Reimpresion" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th style="width:10px"></th>
            <th> Factura # </th>
            <th> Fecha y Hora </th>
            <th> Producto </th>
            <th> Serial </th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
            <tr>
                <td style="width:10px">
                        <div class="col-md-9 col-sm-9 col-xs-12">
                            <div class="">
                                <label>
                                    <input type="checkbox" data-id="@string.Concat(item.Consecutivo,"_",item.IdProducto)" class="flat-sss checkbox-seleccionar-reimpresion">
                                </label>
                            </div>
                        </div>
                    </td>
                
                <td> @Html.DisplayFor(modelItem => item.NumeroFactura) </td>
                <td> @Html.DisplayFor(modelItem => item.FechaCompra) </td>
                <td> @Html.DisplayFor(modelItem => item.NombreProducto) </td>
                <td> @Html.DisplayFor(modelItem => item.Consecutivo) </td>
            </tr>
            }
        }
    </tbody>
</table>

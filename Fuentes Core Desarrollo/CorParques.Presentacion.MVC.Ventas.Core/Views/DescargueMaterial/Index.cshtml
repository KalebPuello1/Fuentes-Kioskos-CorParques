
@{
    ViewBag.Title = "Redención boletería";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@*<form id="frmDescargueBoleteria">*@
    <div class="row">
        <div class="col-md-8">
            <input type="text" id="CodBarraProducto" class="form-control required" placeholder="Código barras" maxlength="13"  />
        </div>
    </div>
    <div class="row">
        <div class="col-md-8 col-sm-8 col-xs-12" id="listproductos">
            @Html.Partial("_ListaProductos", new List<CorParques.Negocio.Entidades.Producto>())
        </div>
    </div>
    <div class="row" style="padding-top: 15px;">
        <div class="col-md-8 col-sm-8 col-xs-12">
            <div style="text-align: right;">
                <input type="button" class="btn btn-primary" value="Descargar" id="btnDescargar" disabled />
                <input type="button" class="btn btn-danger" value="Cancelar" id="btnCancelar" />
            </div>
        </div>
    </div>
@*</form>*@

@section Scripts{
    <script src="~/Vendors/jQuery-Mask-Plugin-master/dist/jquery.mask.min.js"></script>
    <script src="~/Scripts/DescargueMaterial.js"></script>
}

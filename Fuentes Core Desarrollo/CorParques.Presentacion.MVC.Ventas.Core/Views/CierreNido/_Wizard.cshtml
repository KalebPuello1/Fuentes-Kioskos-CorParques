                            <table class="table table-striped table-bordered" cellspacing="0" style="width:100% !Important; margin-left:auto; margin-right:auto;">
                                <thead>
                                    <tr>
                                        <th width="200px">
                                            Tipo
                                        </th>
                                        <th width="200px">
                                            Asignados
                                        </th>
                                        <th width="200px">
                                            Vendidos
                                        </th>
                                        <th width="200px">

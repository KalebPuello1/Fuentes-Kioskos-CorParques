                                    </th>
                                    <th width="200px">
                                        Valor
                                    </th>
                                    <th width="50px">
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model != null)
                                {
                                    int intContador = 0;
                                    foreach (var item in Model.RecoleccionDocumentos)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.FechaCreacion)
                                            </td>
                                            <td>

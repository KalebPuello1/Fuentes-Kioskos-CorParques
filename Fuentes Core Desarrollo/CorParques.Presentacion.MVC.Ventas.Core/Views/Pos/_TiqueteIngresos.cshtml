@model IEnumerable<CorParques.Negocio.Entidades.PasaporteUso>

<form id="frmTiqueteIngreso">
    <div class="row">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
            <label for="">Pasaporte: </label>
        </div>
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
            <select id="listPasaporte" class="form-control required setSelect2">
                <option value="">[Seleccione] </option>
                @foreach (var item in Model)
                {
                    <option value="@item.Id">@item.Nombre</option>
                }
            </select>
        </div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <input type="button" class="btn btn-success" value="Agregar" id="AdicionarTiquete" />
            </div>
        </div>
    </div>
</form>

@model IEnumerable<CorParques.Negocio.Entidades.Puntos>

<form id="frmTiqueteAtraccion">
    <div class="row">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
            <label>Atracción</label>
        </div>
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
            <select id="listAtracciones" class="form-control required setSelect2">
                <option value="">[Seleccione] </option>
                @foreach (var item in Model)
                {
                    <option value="@item.Id">@item.Nombre</option>
                }
            </select>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <input type="button" class="btn btn-success" value="Agregar" id="AdicionarTiqueteAtraccion" />
        </div>
    </div>
    @if (Model != null && Model.Count() > 0){
       <script>
           var listaAtraccion = @Html.Raw(Json.Encode(Model));
        </script>
    }
</form>




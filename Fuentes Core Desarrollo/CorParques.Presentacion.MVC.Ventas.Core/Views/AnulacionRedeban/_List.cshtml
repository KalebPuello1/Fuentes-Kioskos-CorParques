@model IEnumerable<CorParques.Negocio.Entidades.AnulacionFacturaRedeban>

<table id="datatable-Anulaciones" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th style="width:10px"></th>
            <th> Observaciones </th>
            <th> Factura # </th>
            <th> Referencia</th>
            <th> Numero Tarjeta </th>
            <th> Franquicia</th>
            <th> Fecha y Hora </th>
            <th> Punto </th>
            <th> Usuario </th>
            <th> Valor </th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
        <tr>
            <td style="width:10px">
                <div class="col-md-9 col-sm-9 col-xs-12">
                    <div class="">
                        <label>
                            <input  type="checkbox" name="redeban" data-id="@(item.IdLogRedebanRespuesta.ToString()+"_"+ item.NumReferencia.ToString()+"_"+ item.CodigoFactura.ToString())" id="@(item.IdLogRedebanRespuesta.ToString()+"_"+ item.NumReferencia.ToString()+"_"+ item.CodigoFactura.ToString())" class="flat-sss checkbox-seleccionar-anulacion">
                        </label>
                    </div>
                </div>
            </td>
            <td> <input type="text" name="redeban" class="textbox-observacion-anulacion" maxlength="249" id="@("observaciones_"+item.IdLogRedebanRespuesta.ToString()+ "_"+item.NumReferencia.ToString()+"_"+ item.CodigoFactura.ToString() )" disabled="disabled" /> </td>
            <td> @Html.DisplayFor(modelItem => item.CodigoFactura) </td>
            <td> @Html.DisplayFor(modelItem => item.NumReferencia) </td>
            <td> @Html.DisplayFor(modelItem => item.NumeroTarjeta) </td>
            <td> @Html.DisplayFor(modelItem => item.Franquicia) </td>
            <td> @Html.DisplayFor(modelItem => item.Fecha) </td>
            <td> @Html.DisplayFor(modelItem => item.NombrePunto) </td>
            <td> @Html.DisplayFor(modelItem => item.UsuarioCreacion) </td>
            <td> @Html.DisplayFor(modelItem => item.Valor) </td>
        </tr>
            }
        }
    </tbody>
</table>
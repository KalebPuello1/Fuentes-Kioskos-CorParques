@model CorParques.Negocio.Entidades.Procedimiento
<form id="frmPrint">
    <div class="form-horizontal form-label-left" style="height:400px; overflow-y:auto; overflow-x: hidden ">
        <fieldset>
            <legend><b>Caracterización</b></legend>
            <div class="row">
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="IdCategoriaAtencion">
                        Categoría<span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.DropDownListFor(model => model.IdCategoriaAtencion, new SelectList(Model.ListaCategoriaAtencion, "Id", "Nombre"), "Seleccione...", new { @class = "form-control required" })
                    </div>
                </div>
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="IdTipoPaciente">
                        Tipo de paciente<span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.DropDownListFor(model => model.IdTipoPaciente, new SelectList(Model.ListaTipoPaciente, "Id", "Nombre"), "Seleccione...", new { @class = "form-control required" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="Triage">
                        Triage <span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.DropDownListFor(model => model.Triage, new SelectList(Model.ListaTriage, "Id", "Nombre"), "Seleccione...", new { @class = "form-control required" })
                    </div>
                </div>
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="IdCentroMedico">
                        Ubicación <span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.DropDownListFor(model => model.IdCentroMedico, new SelectList(Model.ListaUbicacion, "Id", "Nombre"), "Seleccione...", new { @class = "form-control required" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="FechaIncidente">
                        Fecha de incidente <span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.FechaIncidenteDDMMAAAA, new { @class = "form-control col-md-7 col-xs-12 required calendario", maxlength = 50 })
                    </div>
                </div>

                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="FechaIncidente">
                        Hora de incidente <span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <div class='input-group date dateTime'>
                            @Html.TextBoxFor(model => model.HoraIncidente, new { @class = "form-control required", placeholder = "HH:MM", maxlength = "8" })
                            <span class="input-group-addon">
                                <span class="glyphicon glyphicon-time"></span>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
        </fieldset>
        <fieldset>
            <legend><b>Datos del paciente</b></legend>
            <div class="row">
                <div class="item form-group col-md-6 col-sm-6 col-xs-12" id="div_ListaEmpleados">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="Listaempleado">
                        Empleado
                    </label>
                    <div class="col-md-9 col-sm-6 col-xs-12">
                        @Html.DropDownListFor(model => model.DatosEmpleado, new SelectList(Model.ListaEmpleados, "Valor", "Texto"), "Seleccione...", new { @class = "form-control required setSelect2" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="IdTipoDocumentoPaciente">
                        Tipo de documento <span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.DropDownListFor(model => model.IdTipoDocumentoPaciente, new SelectList(Model.ListaTipoDocumento, "Id", "Nombre"), "Seleccione...", new { @class = "form-control required" })
                    </div>
                </div>

                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="DocumentoPaciente">
                        Número documento <span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.DocumentoPaciente, new { @class = "form-control col-md-7 col-xs-12 required", maxlength = 50 })
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="NombrePaciente">
                        Nombre<span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.NombrePaciente, new { @class = "form-control col-md-7 col-xs-12 required", maxlength = 100 })
                    </div>
                </div>

                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="ApellidoPaciente">
                        Apellido<span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.ApellidoPaciente, new { @class = "form-control col-md-7 col-xs-12 required", maxlength = 100 })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="EdadPaciente">
                        Edad<span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.EdadPaciente, new { @class = "form-control col-md-7 col-xs-12 required", maxlength = 3, onkeypress = "return EsNumero(this);", onchange = "ValidarEdad();" })
                    </div>
                </div>

                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="EdadPaciente">
                        Meses<span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.Meses, new { @class = "form-control col-md-7 col-xs-12 required", maxlength = 3, onkeypress = "return EsNumero(this);", onchange = "ValidarEdad();" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="DireccionPaciente">
                        Dirección
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.DireccionPaciente, new { @class = "form-control col-md-7 col-xs-12", maxlength = 100 })
                    </div>
                </div>

                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="TelefonoPaciente">
                        Celular/Fijo <span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.TelefonoPaciente, new { @class = "form-control col-md-7 col-xs-12 required numerico", maxlength = 30 })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="CorreoPaciente">
                        Email
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.CorreoPaciente, new { @class = "form-control col-md-7 col-xs-12 email", maxlength = 150 })
                    </div>
                </div>
            </div>            
        </fieldset>

        <fieldset>
            <legend><b>Datos del acompañante</b></legend>
            <div class="row">
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="IdTipoDocumentoAcudiente">
                        Tipo de documento
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.DropDownListFor(model => model.IdTipoDocumentoAcudiente, new SelectList(Model.ListaTipoDocumento, "Id", "Nombre"), "Seleccione...", new { @class = "form-control" })
                    </div>
                </div>

                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="DocumentoAcudiente">
                        Número documento
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.DocumentoAcudiente, new { @class = "form-control col-md-7 col-xs-12", maxlength = 50 })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="NombreAcudiente">
                        Nombre
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.NombreAcudiente, new { @class = "form-control col-md-7 col-xs-12", maxlength = 100 })
                    </div>
                </div>

                <div class="item form-group col-md-6 col-sm-6 col-xs-12">
                    <label class="control-label col-md-3 col-sm-3 col-xs-12" for="TelefonoAcudiente">
                        Celular/Fijo
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.TelefonoAcudiente, new { @class = "form-control col-md-7 col-xs-12 numerico", maxlength = 30, onkeypress = "return NumeroYCaracterEspecial(this);" })
                    </div>
                </div>
            </div>
        </fieldset>
        <fieldset>
            <legend><b>Procedimiento</b></legend>
            <div class="row">
                <div class="item form-group col-md-12 col-sm-12 col-xs-12">
                    <label class="control-label col-md-2 col-sm-2 col-xs-2" for="Causa">
                        Motivo consulta <span>*</span>
                    </label>
                    <div class="col-md-1"></div>
                    <div class="col-md-10 col-sm-12 col-xs-12">
                        @Html.TextAreaFor(model => model.Causa, new { @class = "form-control col-md-7 col-xs-12 required", maxlength = 2000, rows = "3" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-12 col-sm-12 col-xs-12">
                    <label class="control-label col-md-2 col-sm-2 col-xs-2" for="Sintomas">
                        Síntomas
                    </label>
                    <div class="col-md-1"></div>
                    <div class="col-md-10 col-sm-12 col-xs-12">
                        @Html.TextAreaFor(model => model.Sintomas, new { @class = "form-control col-md-7 col-xs-12", maxlength = 2000, rows = "3" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-12 col-sm-12 col-xs-12">
                    <label class="control-label col-md-2 col-sm-2 col-xs-2" for="Alergias">
                        Alergias
                    </label>
                    <div class="col-md-1"></div>
                    <div class="col-md-10 col-sm-12 col-xs-12">
                        @Html.TextAreaFor(model => model.Alergias, new { @class = "form-control col-md-7 col-xs-12", maxlength = 2000, rows = "3" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-12 col-sm-12 col-xs-12">
                    <label class="control-label col-md-2 col-sm-2 col-xs-2" for="Tratamiento">
                        Procedimiento realizado
                    </label>
                    <div class="col-md-1"></div>
                    <div class="col-md-10 col-sm-12 col-xs-12">
                        @Html.TextAreaFor(model => model.Tratamiento, new { @class = "form-control col-md-7 col-xs-12", maxlength = 2000, rows = "3" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-12 col-sm-12 col-xs-12">
                    <label class="control-label col-md-2 col-sm-2 col-xs-2" for="Tratamiento">
                        Traslado
                    </label>
                    <div class="col-md-1"></div>
                    <div class="col-md-10 col-sm-12 col-xs-12">
                        <label>
                            @Html.RadioButtonFor(model => model.Traslado, "No", new { @checked = true }) No
                        </label>
                        <label>
                            @Html.RadioButtonFor(model => model.Traslado, "Si") Si
                        </label>
                    </div>
                </div>
            </div>
            <div class="row" id="div_TieneEps" style="display:none;">
                <div class="item form-group col-md-12 col-sm-12 col-xs-12">
                    <label class="control-label col-md-2 col-sm-2 col-xs-2" for="Tratamiento">
                        ¿Tiene EPS?
                    </label>
                    <div class="col-md-1"></div>
                    <div class="col-md-10 col-sm-12 col-xs-12">
                        <label>
                            @Html.RadioButtonFor(model => model.Eps, "No", new { @checked = true }) No
                        </label>
                        <label>
                            @Html.RadioButtonFor(model => model.Eps, "Si") Si
                        </label>
                    </div>
                </div>
            </div>
            <div class="row" id="div_NombreEps" style="display:none;">
                <div class="item form-group col-md-12 col-sm-12 col-xs-12">
                    <label class="control-label col-md-2 col-sm-2 col-xs-2" for="NombreEPS">
                        EPS<span>*</span>
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        @Html.TextBoxFor(model => model.NombreEps, new { @class = "form-control col-md-7 col-xs-12", maxlength = 50 })
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="item form-group col-md-12 col-sm-12 col-xs-12">
                    <label class="control-label col-md-2 col-sm-2 col-xs-2" for="Recomendaciones">
                        Observaciones<span>*</span>
                    </label>
                    <div class="col-md-1"></div>
                    <div class="col-md-10 col-sm-12 col-xs-12">
                        @Html.TextAreaFor(model => model.Recomendaciones, new { @class = "form-control col-md-7 col-xs-12 required", maxlength = 4000, rows = "3" })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="item form-group col-md-12 col-sm-12 col-xs-12">
                    <label class="control-label col-md-2 col-sm-2 col-xs-2" for="NombreMedico">
                        Funcionario <span>*</span>
                    </label>
                    <div class="col-md-1"></div>
                    <div class="col-md-10 col-sm-12 col-xs-12">
                        @Html.TextBoxFor(model => model.NombreMedico, new { @class = "form-control col-md-7 col-xs-12 required", maxlength = 100 })
                    </div>
                </div>
            </div>
        </fieldset>
    </div>
</form>
<script>
    $(function () {
        EventoTipoPaciente();
        Inicializar();
        EstablecerTieneEPS();
        EstablecerNombreEPS();
    });
</script>
<script>
    asignarSelect2();
</script>
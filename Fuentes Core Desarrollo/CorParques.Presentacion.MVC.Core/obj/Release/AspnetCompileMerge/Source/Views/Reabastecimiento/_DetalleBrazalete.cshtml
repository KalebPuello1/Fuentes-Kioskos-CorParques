@model List<CorParques.Negocio.Entidades.BrazaletesApertura>

@if (Model != null && Model.Count() > 0)
{
    <label> Brazaletes</label>

    <table class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>
                    Tipo
                </th>
                <th style="width:15px">
                    Cantidad
                </th>
            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model) {
                <tr>
                    <td>
                        @item.Nombre
                    </td>
                    <td>
                        @item.Cantidad
                    </td>
                </tr>
            }

        </tbody>
    </table>
}


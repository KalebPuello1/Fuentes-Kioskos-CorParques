@model IEnumerable<CorParques.Negocio.Entidades.AnulacionFactura>

<table id="datatable-Anulaciones" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th style="width:10px"></th>
            <th> Observaciones </th>
            <th> Factura # </th>
            <th> Fecha y Hora </th>
            <th> Punto </th>
            <th> Usuario </th>
            <th> Valor </th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td style="width:10px">
                        <div class="col-md-9 col-sm-9 col-xs-12">
                            <div class="">
                                <label>
                                    <input type="checkbox" data-id="@item.IdFactura.ToString()" class="flat-sss checkbox-seleccionar-anulacion">
                                </label>
                            </div>
                        </div>
                    </td>
                    <td> <input type="text" class="textbox-observacion-anulacion" maxlength="249" id="@("observaciones_"+item.IdFactura.ToString())" disabled="disabled" /> </td>
                    <td> @Html.DisplayFor(modelItem => item.CodigoFactura) </td>
                    <td> @Html.DisplayFor(modelItem => item.Fecha) </td>
                    <td> @Html.DisplayFor(modelItem => item.NombrePunto) </td>
                    <td> @Html.DisplayFor(modelItem => item.UsuarioCreacion) </td>
                    <td> @Html.DisplayFor(modelItem => item.Valor) </td>
                </tr>
            }
        }
    </tbody>
</table>
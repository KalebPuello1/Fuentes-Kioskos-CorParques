@model IEnumerable<CorParques.Negocio.Entidades.CodigoFechaAbierta>

<table class="table" id="datatable-responsive">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CodSapPedido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CodBarrasBoletaControl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaInicial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaFinal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Redencion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdSolicitudBoletaControl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaRedencion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdEstado)
        </th>
        <th></th>
    </tr>
    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CodSapPedido)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CodBarrasBoletaControl)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FechaInicial)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FechaFinal)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Redencion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdSolicitudBoletaControl)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FechaRedencion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdEstado)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.IdPedidoBoletaControl }) |
                    @Html.ActionLink("Details", "Details", new { id = item.IdPedidoBoletaControl }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.IdPedidoBoletaControl })
                </td>
            </tr>
        }
    }
</table>

@model IEnumerable<CorParques.Negocio.Entidades.SoporteIT>
@if (Model != null)
{
    if (Model != null && Model.Count() > 0)
    {        
            <table id="datatable-responsive" class="table table-striped table-bordered" cellspacing="0" width="100%">
                <thead>
                    <tr>
                        <th style="width: 100px">Semaforo</th>
                        <th>Observación </th>

              
                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in Model)
                        {
                            
                            <tr>
                                <td>
                                    <div style="background-color:@item.Semaforo; width: 20px; height: 20px; border-radius: 50%;  display: inline-block;">
                                    </div>
                                </td>

                                <td>
                                    @Html.DisplayFor(modelItem => item.Comentario)
                                </td>
                                                
                            </tr>
                        }
                </tbody>
            </table>
            <br />
            
    }
}

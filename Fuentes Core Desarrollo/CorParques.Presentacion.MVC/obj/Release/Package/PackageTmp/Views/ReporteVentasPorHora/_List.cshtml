@{
    var listPuntos = (IEnumerable<CorParques.Negocio.Entidades.TipoGeneral>)ViewBag.Puntos;
    var Productos = (IEnumerable<CorParques.Negocio.Entidades.TipoGeneral>)ViewBag.Productos;
    var centroB = (IEnumerable<CorParques.Negocio.Entidades.TipoGeneral>)ViewBag.centroB;
    
}
<form id="frmVentaHoras">
    <div class="row" id="divFechas">
        <div class="item form-group col-md-6 col-sm-6 col-xs-12">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">
                Fecha Inicial
            </label>
            <div class="col-md-8 col-sm-8 col-xs-12">
                <div class='input-group date required' id='datetimepickerIni'>
                    <input type='text' class="form-control fechaIni required" id="txtFechaInicial" />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
            </div>
        </div>

        <div class="item form-group col-md-6 col-sm-6 col-xs-12">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">
                Fecha Final
            </label>
            <div class="col-md-8 col-sm-8 col-xs-12">
                <div class='input-group date required' id='datetimepickerFin'>
                    <input type='text' class="form-control fechaFin required" id="txtFechaFinal" />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
            </div>
        </div>
    </div>

    <div class="row" id="divHoras">
        <div class="item form-group col-md-6 col-sm-6 col-xs-12">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">
                Hora Inicial
            </label>
            <div class="col-md-8 col-sm-8 col-xs-12">

                <div class='input-group date dateTime'>
                    <input type="text" class="form-control" id="datetimepicker1" />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-time"></span>
                    </span>
                </div>


            </div>
            <input type="hidden" name="HoraInicial" id="HoraInicial">
        </div>

        <div class="item form-group col-md-6 col-sm-6 col-xs-12">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">
                Hora Final
            </label>
            <div class="col-md-8 col-sm-8 col-xs-12">
                <div class='input-group date dateTime'>
                    <input type="text" class="form-control" id="datetimepicker2" />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-time"></span>
                    </span>
                </div>
            </div>
            <input type="hidden" name="HoraFinal" id="HoraFinal">
        </div>
    </div>

    <div class="row">
        <div class="item form-group col-md-6 col-sm-6 col-xs-12">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">
                Punto
            </label>
            <div class="col-md-8 col-sm-8 col-xs-12">
                <select id="DDL_Punto" class="form-control setSelect2">
                    <option value="">Seleccione...</option>
                    @if (listPuntos != null && listPuntos.Count() > 0)
                    {
                        foreach (var item in listPuntos)
                        {
                            <option value="@item.Id">@item.Nombre</option>
                        }
                    }
                </select>
            </div>
        </div>

        <div class="form-group col-md-6 col-sm-6 col-xs-12">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">Producto</label>
            <div class="col-md-8 col-sm-8 col-xs-12">
                <select class="form-control setSelect2" name="Producto" id="Producto">
                    <option value="">Seleccione</option>
                    <option value="null">Todos</option>
                    @foreach (CorParques.Negocio.Entidades.TipoGeneral item in Productos)
                    {
                        <option value="@item.CodSAP">@item.CodSAP - @item.Nombre</option>
                    }
                </select>
            </div>
        </div>
    </div>


    <div class="row">
        <div class="item form-group col-md-6 col-sm-6 col-xs-12">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">
                Tipo de producto
            </label>
            <div class="col-md-8 col-sm-8 col-xs-12">
                <select id="DDL_CB" class="form-control setSelect2">
                    <option value="">Seleccione...</option>
                    @if (centroB != null && centroB.Count() > 0)
                    {
                        foreach (var item in centroB)
                        {
                            <option value="@item.Id">@item.Nombre</option>
                        }
                    }
                </select>
            </div>
        </div>
    </div>


    <div class="row">
        <div class="item form-group col-md-6 col-sm-6 col-xs-12">
        </div>

        <div class="item form-group col-md-6 col-sm-6 col-xs-12">
            <label class="control-label col-md-3 col-sm-3 col-xs-12">
            </label>
            <div class="col-md-12 col-sm-8 col-xs-12">
                <div style="text-align:right;">
                    <button type="button" class="right btn btn-success" id="btnGrid">Vista Previa</button>
                    <a class="btn btn-success" id="btnConsultar">
                        <i class="fa fa-file-excel-o"></i> Exportar a Excel
                    </a>
                    <button type="button" class="right btn btn-danger" id="btnLimpiar">Limpiar</button>
                </div>
            </div>
        </div>
    </div>
    <br />
    <br />
    <div class="row" id="div_grilla"></div>
</form>
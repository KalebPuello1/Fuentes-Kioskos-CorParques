@model IEnumerable<CorParques.Negocio.Entidades.ResolucionFactura>

<table id="datatable-responsive" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th style="width:25px"></th>
            <th>
                Restantes
            </th>
            <th>
                Punto
            </th>
            <th>
                Resolucion
            </th>
            <th>
                Prefijo
            </th>
            <th>
                Cons. Inicial
            </th>
            <th>
                Cons. Final
            </th>
            <th>
                Fecha Inicio
            </th>
            <th>
                Fecha Final
            </th>
            <th>
                Ultima Factura
            </th>
            <th style="width:50px"></th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
        <tr>
            <td>
                <div style="background-color:@(item.Restante>int.Parse(System.Configuration.ConfigurationManager.AppSettings["rangos"].Split(',')[0])?"green":item.Restante<int.Parse(System.Configuration.ConfigurationManager.AppSettings["rangos"].Split(',')[1])?"red":"yellow"); width: 20px; height: 20px; border-radius: 50%;  display: inline-block;">
                </div>               
            </td>
            <td>                
                @Html.DisplayFor(modelItem => item.Restante)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombrePunto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Resolucion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prefijo)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.ConsecutivoInicial)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.ConsecutivoFinal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFinal)
            </td>
            <td>
                <label>@item.UltimaFactura</label>

            </td>
            <td style="text-align:center; width:50px">
                @if (ViewBag.Aprovador)
                {
                    if (item.IdEstado == 7)
                    {
                        <a class="lnkApproved" data-id="@item.IdResolucion" href="javascript:void(0)"><b class="fa fa-arrow-circle-o-up"></b></a>
                    }
                }
                <a class="lnkDisable" data-id="@item.IdResolucion" href="javascript:void(0)"><b class="fa fa-times-circle"></b></a>
            </td>
        </tr>
            }
        }

    </tbody>
</table>
<script>
        var listado = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model));
</script>
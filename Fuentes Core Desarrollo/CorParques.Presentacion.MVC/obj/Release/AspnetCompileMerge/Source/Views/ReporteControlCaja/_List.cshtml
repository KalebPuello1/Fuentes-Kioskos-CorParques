@{
    var Perfiles = (IEnumerable<CorParques.Negocio.Entidades.Perfil>)ViewBag.Perfiles;
    var Taquillero = (IEnumerable<CorParques.Negocio.Entidades.TipoGeneral>)ViewBag.Taquilleros;
}

<div class="form-group col-md-6 col-sm-6 col-xs-12">
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Fecha Inicial</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <div class="input-group date" id="datetimepickerIni">
            <input type="text" class="form-control FInicial required" id="FInicial">
            <span class="input-group-addon">
                <span class="glyphicon glyphicon-calendar"></span>
            </span>
        </div>
    </div>
</div>

<div class="form-group col-md-6 col-sm-6 col-xs-12">
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Fecha Final</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <div class="input-group date" id="datetimepickerFin">
            <input type="text" class="form-control FFinnal required" id="FFinnal">
            <span class="input-group-addon">
                <span class="glyphicon glyphicon-calendar"></span>
            </span>
        </div>
    </div>
</div>
<!--##################-->
<div class="form-group col-md-6 col-sm-6 col-xs-12">
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Perfiles</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <select class="form-control setSelect2" name="idPerfil" id="idPerfil">
            <option value="">* Seleccione</option>
            @foreach (CorParques.Negocio.Entidades.Perfil item in Perfiles)
            {
                <option value="@item.IdPerfil">@item.Nombre</option>
            }
        </select>
    </div>
</div>
<!--##################-->
<div class="form-group col-md-6 col-sm-6 col-xs-12">
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Taquillero</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <select class="form-control setSelect2" name="idTaquillero" id="idTaquillero">
            <option value="">* Seleccione</option>
            @foreach (CorParques.Negocio.Entidades.TipoGeneral item in Taquillero)
            {
                <option value="@item.Id">@(item.CodSAP + " - " + item.Nombre)   </option>
            }
        </select>
    </div>
</div>

<div class="form-group col-md-12 col-sm-12 col-xs-12" style="text-align:right; margin-top:20px;">
    <a href="javascript:void(0)" id="btnConsultar" class="right btn btn-success">Consultar</a>
    <a href="javascript:void(0)" id="btnLimpiar" class="right btn btn-danger">Limpiar</a>
</div>
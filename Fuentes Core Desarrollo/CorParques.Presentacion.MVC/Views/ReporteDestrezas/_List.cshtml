@{
    var listPuntos = (IEnumerable<CorParques.Negocio.Entidades.TipoGeneral>)ViewBag.Puntos;
    var listTipoBoleta = (IEnumerable<CorParques.Negocio.Entidades.TipoGeneral>)ViewBag.TipoBoleta;
    var listClientes = (IEnumerable<CorParques.Negocio.Entidades.TipoGeneral>)ViewBag.Clientes;    
}

<div class="row" id="divFechas">
    <div class="item form-group col-md-6 col-sm-6 col-xs-12">
        <label class="control-label col-md-3 col-sm-3 col-xs-12">
            Fecha Inicial
        </label>
        <div class="col-md-9 col-sm-9 col-xs-12">
            <div class='input-group date' id='datetimepickerIni'>
                <input type='text' class="form-control fechaIni required" id="txtFechaInicial" />
                <span class="input-group-addon">
                    <span class="glyphicon glyphicon-calendar"></span>
                </span>
            </div>
        </div>
    </div>

    <div class="item form-group col-md-6 col-sm-6 col-xs-12">
        <label class="control-label col-md-3 col-sm-3 col-xs-12">
            Fecha Final
        </label>
        <div class="col-md-9 col-sm-9 col-xs-12">
            <div class='input-group date' id='datetimepickerFin'>
                <input type='text' class="form-control fechaFin required" id="txtFechaFinal" />
                <span class="input-group-addon">
                    <span class="glyphicon glyphicon-calendar"></span>
                </span>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="item form-group col-md-6 col-sm-6 col-xs-12">
        <label class="control-label col-md-3 col-sm-3 col-xs-12">
            Punto
        </label>
        <div class="col-md-9 col-sm-9 col-xs-12">
            <select id="selectPunto" class="form-control setSelect2">
                <option value="">Seleccione...</option>
                @if (listPuntos != null && listPuntos.Count() > 0)
                {
                    foreach (var item in listPuntos)
                    {
                        <option value="@item.Id">@item.Nombre</option>
                    }
                }
            </select>
        </div>
    </div>
    <div class="form-group col-md-6 col-sm-6 col-xs-12">
        <label class="control-label col-md-3 col-sm-3 col-xs-12">Tipo Venta</label>
        <div class="col-md-9 col-sm-9 col-xs-12">
            <select id="ddlTipoVenta" class="form-control setSelect2">
                <option value="">Seleccione...</option>
                <option value="1">Uno a uno</option>
                <option value="0">Institucional</option>
            </select>
        </div>
    </div>

    @*<div class="form-group col-md-6 col-sm-6 col-xs-12">
        <label class="control-label col-md-3 col-sm-3 col-xs-12">Series</label>
        <div class="col-md-9 col-sm-9 col-xs-12">
            <select class="form-control setSelect2" name="selectSeries" id="selectSeries">
                <option value="">Seleccione</option>
                @if (listProductos != null && listProductos.Count() > 0)
                {
                    foreach (var item in listProductos)
                    {
                        <option value="@item.IdProducto">@item.Nombre</option>
                    }
                }
            </select>
        </div>
    </div>*@
</div>

<div class="row">
    <div class="form-group col-md-6 col-sm-6 col-xs-12">
        <label class="control-label col-md-3 col-sm-3 col-xs-12">Tipo Boleta</label>
        <div class="col-md-9 col-sm-9 col-xs-12">
            <select id="selectTipoBoleta" class="form-control setSelect2">
                <option value="">Seleccione...</option>
                @if (listTipoBoleta != null && listTipoBoleta.Count() > 0)
                {
                    foreach (var item in listTipoBoleta)
                    {
                        <option value="@item.CodSAP">@item.Nombre</option>
                    }
                }
            </select>
        </div>
        @*<div class="col-md-9 col-sm-9 col-xs-12">
            <input type="text" class="form-control" id="txtTipoBoleta" />
        </div>*@
    </div>

    <div class="item form-group col-md-6 col-sm-6 col-xs-12">
        <label class="control-label col-md-3 col-sm-3 col-xs-12">
            Cliente
        </label>
        <div class="col-md-9 col-sm-9 col-xs-12">
            <select id="selectCliente" class="form-control setSelect2">
                <option value="">Seleccione...</option>
                @if (listClientes != null && listClientes.Count() > 0)
                {
                    foreach (var item in listClientes)
                    {
                        <option value="@item.CodSAP">@item.Nombre</option>
                    }
                }
            </select>
        </div>
        @*<div class="col-md-9 col-sm-9 col-xs-12">
            <input type="text" class="form-control" id="txtCliente" />
        </div>*@
    </div>
</div>



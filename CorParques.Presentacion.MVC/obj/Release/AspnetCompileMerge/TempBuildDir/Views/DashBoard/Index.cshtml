            GenerarGraficoDona(Valores11, 'puntos_ayb_todos')
            GenerarGraficoDona(Valores12, 'mundo_natural_12');
            //GenerarBarras(Valores15, 'destrezas_todos', 'Total')
            //GenerarBarras(Valores20, 'atracciones_por_brazalete', 'Cantidad')

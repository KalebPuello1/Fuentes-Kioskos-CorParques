@model IEnumerable<CorParques.Negocio.Entidades.DashBoard>

@{
    ViewBag.Title = "Dashboard";
    ViewBag.SubTitle = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet" />
<link href="~/Vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">
<link href="~/Vendors/datatables.net-responsive-bs/css/responsive.bootstrap.min.css" rel="stylesheet">
<link href="~/Vendors/datatables.net-scroller-bs/css/scroller.bootstrap.min.css" rel="stylesheet">
<link href="~/Content/daterangepicker.css" rel="stylesheet" />
<div class="row">
    <div id="div_boton_Bitacora" class="pull-left" style="display:none;">
        <input type="button" class="right btn" value="Bitácora" id="btnMostrarBitacora" />
    </div>

    <div class="left_col">
        <div id="reportrange" class="pull-right" style="background: #fff; cursor: pointer; padding: 5px 10px; border: 1px solid #ccc">
            <i class="glyphicon glyphicon-calendar fa fa-calendar"></i>
            <span>Noviembre 01, 2017 - Noviembre 01, 2017</span> <b class="caret"></b>
        </div>
    </div>
</div>
@*<div class="x_content">*@
<div id="listView">
    @Html.Partial("_Reporte")
</div>
@*</div>*@


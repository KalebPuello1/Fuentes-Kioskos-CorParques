@model CorParques.Negocio.Entidades.GestionMantenimientoControl


<form>
    <div class="form-horizontal form-label-left">
        <div class="item form-group">
            <div class="col-md-6 col-sm-6 col-xs-12">
                <div id="dropzoneElement" class="dropzone"></div>
                @Html.HiddenFor(model => model.Imagen)
            </div>
            <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="checkbox">
                    @if (ViewBag.Mantenimientos != null)
                    { 
                        foreach (var item in ViewBag.Mantenimientos)
                        {
                            <label>
                                <input class="flat" name="chkatracion" type="checkbox" value="@item.Id" onchange="selectcheck(this);" />@item.Nombre
                            </label>
                            <br />
                        }
                    }
                </div>

            </div>
        </div>
    </div>   
    @Html.HiddenFor(model => model.Verificadas, new { id = "hdf_atraccionesSeleccionadas", value = "" })
</form>

@{
    ViewBag.Title = "Reporte movimiento inventario";
    var Material = (IEnumerable<CorParques.Negocio.Entidades.Materiales>)ViewBag.Material;
    //var Puntos = (IEnumerable<CorParques.Negocio.Entidades.Puntos>)ViewBag.puntos;
    var Puntos = (IEnumerable<CorParques.Negocio.Entidades.Almacen>)ViewBag.puntos;
    var Usuarios = (IEnumerable<CorParques.Negocio.Entidades.Usuario>)ViewBag.Usuario;
    var UnidadMedida = (List<string>)ViewBag.UnidadMedida;
    var TiposMovimiento = (IEnumerable<CorParques.Negocio.Entidades.TipoMovimiento>)ViewBag.TiposMovimiento;
}


<div class="form-group col-md-6 col-sm-6 col-xs-12">
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Fecha Inicial</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <div class="input-group date" id="datetimepickerIni">
            <input type="text" class="form-control FInicial required" id="FInicial">
            <span class="input-group-addon">
                <span class="glyphicon glyphicon-calendar"></span>
            </span>
        </div>
    </div>
</div>

<div class="form-group col-md-6 col-sm-6 col-xs-12">
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Fecha Final</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <div class="input-group date" id="datetimepickerFin">
            <input type="text" class="form-control FFinnal required" id="FFinal">
            <span class="input-group-addon">
                <span class="glyphicon glyphicon-calendar"></span>
            </span>
        </div>
    </div>
</div>

@*#####################################################################################*@





<div class="form-group col-md-6 col-sm-6 col-xs-12">
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Material</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <select class="form-control setSelect2" name="idMaterial" id="idMaterial">
            <option value="">* Seleccione</option>
            @foreach (CorParques.Negocio.Entidades.Materiales item in Material)
            {
                <option value="@item.CodigoSap">@string.Concat(item.CodigoSap ?? "", " | ", item.Nombre, " ")</option>
            }
        </select>
    </div>
</div>

<div class="form-group col-md-6 col-sm-6 col-xs-12">
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Tipo de movimiento</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <select class="form-control setSelect2" name="IdTipoMovimiento" id="IdTipoMovimiento">
            <option value="">* Seleccione</option>
            @foreach (var item in TiposMovimiento)
            {
                <option value="@item.Id">@item.Descripcion</option>
            }
        </select>
    </div>
</div>




@*#######################################################################################*@


<div class="form-group col-md-6 col-sm-6 col-xs-12">
    <!--<label class="control-label col-md-4 col-sm-4 col-xs-4">Punto origen</label>-->
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Almacen origen</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <select class="form-control setSelect2" name="IdPuntoOrigen" id="IdPuntoOrigen">
            <option value="">* Seleccione</option>
            @foreach (var item in Puntos)
            {
                <option value="@item.CodSapAlmacen">@item.CodSapAlmacen</option>
            }
        </select>
    </div>
</div>

<div class="form-group col-md-6 col-sm-6 col-xs-12">
    @*<label class="control-label col-md-4 col-sm-4 col-xs-4">Punto destino</label>*@
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Almacen destino</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <select class="form-control setSelect2" name="IdPuntoDestino" id="IdPuntoDestino">
            <option value="">* Seleccione</option>
            @foreach (var item in Puntos)
            {
                <option value="@item.CodSapAlmacen">@item.CodSapAlmacen</option>
            }
        </select>
    </div>
</div>

@*#######################################################################################*@

@*#######################################################################################*@



<div class="form-group col-md-6 col-sm-6 col-xs-12">
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Responsable</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <select class="form-control setSelect2" name="IdResponsable" id="IdResponsable">
            <option value="">* Seleccione</option>
            @foreach (var item in Usuarios)
            {
                <option value="@item.Id">@string.Concat(item.Nombre, " ",item.Apellido) </option>
            }

        </select>
    </div>
</div>

<div class="form-group col-md-6 col-sm-6 col-xs-12">
    <label class="control-label col-md-4 col-sm-4 col-xs-4">Unidad de medida</label>
    <div class="col-md-8 col-sm-8 col-xs-8">
        <select class="form-control setSelect2" name="idUnidadMedida" id="idUnidadMedida">
            <option value="">* Seleccione</option>
            @foreach (var item in UnidadMedida)
            {
                <option value="@item">@item</option>
            }
        </select>
    </div>
</div>

@*#######################################################################################*@


<div class="form-group col-md-12 col-sm-12 col-xs-12" style="text-align:right; margin-top:20px;">
    <a href="javascript:void(0)" id="btnConsultar" class="right btn btn-success">Consultar</a>
    <a href="javascript:void(0)" id="btnLimpiar" class="right btn btn-danger">Limpiar</a>
</div>
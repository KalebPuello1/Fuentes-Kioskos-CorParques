
@model IEnumerable<CorParques.Negocio.Entidades.TipoAcompanamiento>

<table id="datatable-responsive" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>
                Nombre
            </th>
            <th>
                Estado
            </th>
            <th style="width:50px"></th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
                <tr>                
                    <td>
                        @Html.DisplayFor(modelItem => item.Nombre)
                    </td>
                    <td>
                        @((CorParques.Transversales.Util.Enumerador.Estados)item.Estado)
                    </td>
                   
                    <td style="text-align:center; width:50px">
                        <a class="lnkEditTipoAcompaAdmin" data-id="@item.Id" href="javascript:void(0)"><b class="fa fa-pencil-square"></b></a>
                        <a class="lnkDeleteTipoAcompa" data-id="@item.Id" href="javascript:void(0)"><b class="fa fa-times-circle"></b></a>
                        @*<a class="lnkDelete" data-id="@item.IdProducto" href="javascript:void(0)"><b class="fa fa-times-circle"></b></a>*@
                    </td>
                </tr>
            }
        }

    </tbody>
</table>

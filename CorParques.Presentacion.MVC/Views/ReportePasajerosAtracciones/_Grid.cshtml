@model IEnumerable<CorParques.Negocio.Entidades.ReportePasajeros>

<div class="row" id="div_tabla">
    <table id="datatable-responsive" class="table table-striped table-bordered" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>
                    Fecha
                </th>
                <th>
                    Punto
                </th>
                <th>
                    Tipo de producto
                </th>
                <th>
                    Producto
                </th>
                <th>
                    Cantidad Usos
                </th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Fecha)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Atraccion)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TipoProducto)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Producto)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CantidadUsos)
                        </td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
